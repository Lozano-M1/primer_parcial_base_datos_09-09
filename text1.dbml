Table Users {
  id_users serial [pk]
  username varchar [unique]
  email varchar
  contrasena varchar(10)
}
Table Planes {
  id_planes   int
  id_users_planes int
  nombre_planes varchar
  precio_usd  decimal
}
Table Suscripciones {
  id_suscripciones  int
  usuario  int
  plan        int
  fecha_inicio date
  fecha_fin   date
}
Table Pagos {
  id_pagos       int
  suscripcion_hd int
  monto_usd   decimal
  estado      varchar
  pagado_en   timestamp
}
Table EstadoPago {
  id_estado_pago  int
  completado varchar
}
Table Perfiles {
  id_perfiles         int
  usuario  int
  nombre_perfil      varchar
  programas     varchar
  edad        int
  idioma      varchar
  historial   varchar

}
Table Busqueda {
  id_titulos          int
  nombre_titulo       varchar
  tipo                varchar   // pel√≠cula o serie
  generos             varchar
  sinopsis            text
  ano_estreno         int
}
Table Tipos {
  id_tipos  int
  tipo_d_programa varchar
  peliculas varchar
  series    varchar
}

Table Generos {
  id_generos int
  busqueda_d_genero int
  terror   varchar
  accion   varchar
  romance varchar
  anime varchar
  risa varchar
  documentales varchar
}
Table Historial {
  id_historial          int
  titulo   int
  episodio   int
  visto_en   varchar
}

Ref: Users.id_users < Planes.id_users_planes
Ref: Users.username < Perfiles.usuario
Ref: Planes.precio_usd < Suscripciones.plan
Ref: Suscripciones.usuario < Pagos.suscripcion_hd
Ref: Pagos.estado < EstadoPago.completado
Ref: Perfiles.programas < Busqueda.nombre_titulo
Ref: Busqueda.tipo < Tipos.tipo_d_programa
Ref: Busqueda.generos < Generos.busqueda_d_genero
Ref: Perfiles.historial < Historial.titulo